<Configuration>
    <Appenders>
        <Console name="OUT" target="SYSTEM_OUT">
        <PatternLayout pattern="%d{HH:mm:ss.SSS} [%t] %-5level %logger{36} - %msg%n"/>
        </Console>
    </Appenders>
    <Loggers>
        <Root level="trace">
        <AppenderRef ref="OUT"/>
        </Root>
    </Loggers>
</Configuration>


